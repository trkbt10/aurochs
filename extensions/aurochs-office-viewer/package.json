{
  "name": "aurochs-office-viewer",
  "displayName": "aurochs Office Viewer",
  "description": "View PPTX, PPT, DOCX, DOC, XLSX, and XLS files directly in VS Code",
  "version": "0.2.0",
  "publisher": "trkbt10",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trkbt10/aurochs",
    "directory": "extensions/aurochs-office-viewer"
  },
  "type": "module",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./dist/extension.js",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "contributes": {
    "customEditors": [
      {
        "viewType": "aurochs.pptxViewer",
        "displayName": "PPTX Viewer",
        "selector": [
          {
            "filenamePattern": "*.pptx"
          },
          {
            "filenamePattern": "*.ppt"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "aurochs.docxViewer",
        "displayName": "DOCX Viewer",
        "selector": [
          {
            "filenamePattern": "*.docx"
          },
          {
            "filenamePattern": "*.doc"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "aurochs.xlsxViewer",
        "displayName": "XLSX Viewer",
        "selector": [
          {
            "filenamePattern": "*.xlsx"
          },
          {
            "filenamePattern": "*.xls"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.2.2"
  }
}
