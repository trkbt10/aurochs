{
  "name": "@aurochs-ui/ui-preview",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "description": "Development preview server for @aurochs-ui packages",
  "dependencies": {
    "@aurochs-builder/xlsx": "workspace:*",
    "@aurochs-office/docx": "workspace:*",
    "@aurochs-office/drawing-ml": "workspace:*",
    "@aurochs-office/opc": "workspace:*",
    "@aurochs-office/pptx": "workspace:*",
    "@aurochs-renderer/pptx": "workspace:*",
    "@aurochs-office/xls": "workspace:*",
    "@aurochs-office/xlsx": "workspace:*",
    "@aurochs-ui/ui-components": "workspace:*",
    "@aurochs-ui/editor-controls": "workspace:*",
    "@aurochs-ui/docx-editor": "workspace:*",
    "@aurochs-ui/pptx-editor": "workspace:*",
    "@aurochs-ui/xlsx-editor": "workspace:*",
    "react-router-dom": "^7.6.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "dev:xlsx": "vite --open /xlsx-editor.html",
    "dev:docx": "vite --open /docx-editor.html",
    "dev:pptx-slideshow": "vite --open /pptx-slideshow.html",
    "dev:pptx-viewer": "vite --open /pptx-viewer.html",
    "dev:editor-controls": "vite --open /editor-controls.html",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
