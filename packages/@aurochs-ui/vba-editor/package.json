{
  "name": "@aurochs-ui/vba-editor",
  "version": "0.5.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@aurochs-office/vba": "workspace:*",
    "@aurochs-ui/editor-core": "workspace:*",
    "@aurochs-ui/editor-controls": "workspace:*",
    "@aurochs-ui/ui-components": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --passWithNoTests --config ../../../vitest.config.ts '../../../packages/@aurochs-ui/vba-editor/src'",
