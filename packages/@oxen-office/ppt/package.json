{
  "name": "@oxen-office/ppt",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@oxen-office/cfb": "workspace:*",
    "@oxen-office/pptx": "workspace:*",
    "@oxen/zip": "workspace:*"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest --run --passWithNoTests --config ../../../vitest.config.ts '../../../packages/@oxen-office/ppt/src'"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./converter": {
      "types": "./src/converter/index.ts",
      "default": "./src/converter/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./*/*": {
      "types": "./src/*/*.ts",
      "default": "./src/*/*.ts"
    },
    "./*/*/*": {
      "types": "./src/*/*/*.ts",
      "default": "./src/*/*/*.ts"
    }
  }
}
