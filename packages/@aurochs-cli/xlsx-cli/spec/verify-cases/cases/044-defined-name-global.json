{
  "name": "defined-name-global",
  "description": "Global defined name TotalRange referencing Sheet1!$A$1:$A$10",
  "tags": ["defined-names"],
  "input": {
    "mode": "create",
    "output": "../__output__/defined-name-global.xlsx",
    "workbook": {
      "sheets": [
        {
          "name": "Sheet1",
          "rows": [
            {
              "row": 1,
              "cells": [
                { "ref": "A1", "value": 100 }
              ]
            }
          ]
        }
      ],
      "definedNames": [
        { "name": "TotalRange", "formula": "Sheet1!$A$1:$A$10" }
      ]
    }
  },
  "expected": {
    "sheetCount": 1,
    "totalCells": 1,
    "definedNames": [
      { "name": "TotalRange", "formula": "Sheet1!$A$1:$A$10" }
    ],
    "sheets": [
      {
        "name": "Sheet1",
        "rowCount": 1,
        "cellCount": 1,
        "cells": [
          { "ref": "A1", "type": "number", "value": 100 }
        ]
      }
    ]
  }
}
