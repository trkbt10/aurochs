{
  "name": "merge-with-data",
  "description": "Merge A1:B2 with Title, plus separate data cells C1 and C2",
  "tags": ["merge"],
  "input": {
    "mode": "create",
    "output": "../__output__/merge-with-data.xlsx",
    "workbook": {
      "sheets": [
        {
          "name": "Sheet1",
          "mergeCells": ["A1:B2"],
          "rows": [
            {
              "row": 1,
              "cells": [
                { "ref": "A1", "value": "Title" },
                { "ref": "C1", "value": 10 }
              ]
            },
            {
              "row": 2,
              "cells": [
                { "ref": "C2", "value": 20 }
              ]
            }
          ]
        }
      ]
    }
  },
  "expected": {
    "sheetCount": 1,
    "totalCells": 3,
    "sheets": [
      {
        "name": "Sheet1",
        "rowCount": 2,
        "cellCount": 3,
        "mergedCells": ["A1:B2"],
        "cells": [
          { "ref": "A1", "type": "string", "value": "Title" },
          { "ref": "C1", "type": "number", "value": 10 },
          { "ref": "C2", "type": "number", "value": 20 }
        ]
      }
    ]
  }
}
