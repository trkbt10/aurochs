{
  "name": "modify-defined-names",
  "description": "Add and modify defined names in modify mode",
  "tags": ["modify", "definedNames"],
  "setup": {
    "mode": "create",
    "output": "../__output__/065-template.xlsx",
    "workbook": {
      "sheets": [
        {
          "name": "Data",
          "rows": [
            { "row": 1, "cells": [{ "ref": "A1", "value": 10 }, { "ref": "B1", "value": 20 }] }
          ]
        }
      ],
      "definedNames": [
        { "name": "OldRange", "formula": "Data!$A$1" }
      ]
    }
  },
  "input": {
    "mode": "modify",
    "template": "../__output__/065-template.xlsx",
    "output": "../__output__/065-modify-names.xlsx",
    "modify": {
      "definedNames": [
        { "name": "OldRange", "formula": "Data!$A$1:$B$1" },
        { "name": "NewRange", "formula": "Data!$A$1:$A$10" }
      ]
    }
  },
  "expected": {
    "sheetCount": 1,
    "definedNames": [
      { "name": "OldRange", "formula": "Data!$A$1:$B$1" },
      { "name": "NewRange", "formula": "Data!$A$1:$A$10" }
    ]
  }
}
