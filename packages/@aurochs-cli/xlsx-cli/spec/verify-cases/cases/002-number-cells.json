{
  "name": "number-cells",
  "description": "Basic number cell values including integer, decimal, and negative",
  "tags": ["cell-values"],
  "input": {
    "mode": "create",
    "output": "../__output__/002-number-cells.xlsx",
    "workbook": {
      "sheets": [
        {
          "name": "Sheet1",
          "rows": [
            { "row": 1, "cells": [{ "ref": "A1", "value": 42 }, { "ref": "B1", "value": 3.14 }, { "ref": "C1", "value": -100 }] }
          ]
        }
      ]
    }
  },
  "expected": {
    "sheetCount": 1,
    "sheetNames": ["Sheet1"],
    "totalRows": 1,
    "totalCells": 3,
    "sheets": [
      {
        "name": "Sheet1",
        "cells": [
          { "ref": "A1", "type": "number", "value": 42 },
          { "ref": "B1", "type": "number", "value": 3.14 },
          { "ref": "C1", "type": "number", "value": -100 }
        ]
      }
    ]
  }
}
